    "author": {
        "name": "Sivan Wolberg",
        "email": "sivan@wolberg.pro"
    },
    "scripts": {
        "dev": "next dev",
        "dev:watch": "cross-env NODE_OPTIONS='--inspect' next dev",
        "build": "next build && tsc --project tsconfig.server.json",
        "start": "cross-env NODE_ENV=production node .build/server",
        "start:debug": "cross-env NODE_ENV=debug node .build/server",
        "start:staging": "cross-env NODE_ENV=staging node .build/server",
        "analyze": "cross-env ANALYZE=1 next build && cat .next/stats.txt",
        "type-check": "tsc --pretty --noEmit",
        "format": "prettier --write .",
        "lint": "eslint --config .eslintrc --ext ts --ext tsx --ext js",
        "test": "jest",
        "test-all": "yarn lint && yarn type-check && yarn test",
        "postbuild": "next-sitemap",
        "storybook": "start-storybook -p 6006 --no-dll",
        "build-storybook": "build-storybook --no-dll"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "yarn run type-check"
        }
    },
    "lint-staged": {
        "*.@(ts|tsx)": [
            "yarn lint",
            "yarn format"
        ]
    },
    "dependencies": {
        "@next/bundle-analyzer": "^9.1.4",
        "@types/lodash": "^4.14.170",
        "@types/redux-actions": "^2.6.1",
        "@types/redux-devtools": "^3.7.0",
