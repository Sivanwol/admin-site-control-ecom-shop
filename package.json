        "express": "^4.17.1",
        "history": "^5.0.0",
        "lodash": "^4.17.21",
        "next": "11.0.1",
        "next-i18next": "^8.5.3",
        "next-intl": "^1.0.1",
        "next-redux-wrapper": "~1.3.2",
        "nprogress": "^0.2.0",
        "postcss-flexbugs-fixes": "^5.0.2",
        "postcss-preset-env": "^6.7.0",
        "primeflex": "^2.0.0",
        "primeicons": "^4.1.0",
        "primereact": "^6.5.0-rc.2",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-helmet": "^6.0.0",
        "react-redux": "^7.1.0",
        "react-redux-annotation": "^1.0.10",
        "react-transition-group": "^4.4.2",
        "redux": "^4.0.4",
        "redux-actions": "^2.6.5",
        "redux-thunk": "^2.3.0",
        "sass": "1.26.3",
        "superagent": "^6.1.0",
        "url-join": "^4.0.1"
    },
    "devDependencies": {
        "@compiled/react": "^0.6.7",
        "@compiled/webpack-loader": "^0.6.7",
        "@playwright/test": "^1.12.3",
        "@storybook/addon-actions": "^6.1.11",
        "@storybook/addon-essentials": "^6.1.11",
        "@storybook/addon-links": "^6.1.11",
        "@storybook/react": "^6.1.11",
        "@styled-jsx/plugin-sass": "^3.0.0",
        "@testing-library/jest-dom": "^5.0.0",
        "@testing-library/react": "^11.0.0",
        "@testing-library/user-event": "^13.0.0",
        "@types/jest": "^26.0.23",
        "@types/node": "^14.14.25",
        "@types/react": "17.0.13",
        "@types/redux-logger": "^3.0.8",
        "@typescript-eslint/eslint-plugin": "^4.14.2",
        "@typescript-eslint/parser": "^4.14.2",
        "babel-jest": "^27.0.2",
        "babel-loader": "^8.1.0",
        "eslint": "7.30.0",
