    "scripts": {
        "dev": "nodemon --exec ts-node --project tsconfig.server.json src/server",
        "dev:watch": "cross-env NODE_OPTIONS='--inspect' next dev",
        "dev:debug": "concurrently -r \"yarn run dev:watch\" \"npx graphql-codegen --watch\"",
        "build": "next build && tsc --project tsconfig.server.json && yarn run codegen",
        "start": "cross-env NODE_ENV=production node .build/server",
        "start:test": "cross-env NODE_ENV=staging node .build/server",
        "start:staging": "cross-env NODE_ENV=staging node .build/server",
        "start:prod": "yarn run build && yarn run test:all && cross-env NODE_ENV=production node .build/server",
        "analyze": "cross-env ANALYZE=1 next build && cat .next/stats.txt",
        "type-check": "tsc --pretty --noEmit",
        "format": "prettier --write .",
        "lint": "eslint --config .eslintrc --ext ts --ext tsx --ext js",
        "test:ci": "jest",
        "test:all": "yarn lint && yarn type-check && yarn test",
        "test": "jest --watch",
        "codegen": "graphql-codegen",
        "postbuild": "next-sitemap --config next-sitemap.config.js",
        "storybook": "start-storybook -p 6006 --no-dll",
        "build-storybook": "build-storybook --no-dll",
        "generate": "yarn run generate:prisma && yarn run generate:nexus",
        "generate:prisma": "prisma generate",
        "generate:nexus": "npx graphql-codegen"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "yarn run type-check"
        }
    },
    "lint-staged": {
        "*.@(ts|tsx)": [
            "yarn lint",
            "yarn format"
        ]
    },
